  },
  "dependencies": {
    "@deepkit/api-console-module": "^1.0.1-alpha.43",
    "@deepkit/app": "^1.0.1-alpha.43",
    "@deepkit/broker": "^1.0.1-alpha.43",
    "@deepkit/bson": "^1.0.1-alpha.43",
    "@deepkit/core": "^1.0.1-alpha.43",
    "@deepkit/core-rxjs": "^1.0.1-alpha.43",
    "@deepkit/crypto": "^1.0.1-alpha.35",
    "@deepkit/framework": "^1.0.1-alpha.43",
    "@deepkit/http": "^1.0.1-alpha.43",
    "@deepkit/injector": "^1.0.1-alpha.43",
    "@deepkit/logger": "^1.0.1-alpha.43",
    "@deepkit/rpc": "^1.0.1-alpha.43",
    "@deepkit/rpc-tcp": "^1.0.1-alpha.43",
    "@deepkit/type": "^1.0.1-alpha.43",
    "@deepkit/workflow": "^1.0.1-alpha.43",
    "@deepkit/event": "^1.0.1-alpha.43",
    "@deepkit/orm": "^1.0.1-alpha.43",
    "@deepkit/stopwatch": "^1.0.1-alpha.43",
    "@deepkit/template": "^1.0.1-alpha.43",
    "@deepkit/sql": "^1.0.1-alpha.43",
    "@deepkit/sqlite": "^1.0.1-alpha.43",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
