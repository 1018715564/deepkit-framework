{
  "name": "@super-hornet/topsort",
  "version": "0.0.1",
  "description": "High-Performance topological sort / dependency resolver with optional grouping algorithm for Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "jest --coverage",
    "tsc": "tsc",
    "tsc-watch": "rm -rf dist && tsc --watch"
  },
  "repository": "https://github.com/super-hornet/super-hornet.ts",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/node_modules/**",
      "!**/lib/**/*",
      "!**/*.d.ts",
      "!index.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
