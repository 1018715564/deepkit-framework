{
  "name": "@deepkit/rpc",
  "version": "1.0.1-alpha.12",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json"
  },
  "peerDependencies": {
    "@deepkit/type": "^1.0.1-alpha.0",
    "rxjs": "~6.6.0"
  },
  "dependencies": {
    "@deepkit/bson": "^1.0.1-alpha.12",
    "@deepkit/core": "^1.0.1-alpha.12",
    "@deepkit/core-rxjs": "^1.0.1-alpha.12",
    "@deepkit/type": "^1.0.1-alpha.12",
    "dot-prop": "^5.1.1"
  },
  "devDependencies": {
    "@types/dot-prop": "^5.0.1",
    "@types/fs-extra": "^9.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  }
}
