    "@deepkit/marshal-sql": "^1.0.0-alpha.0",
    "sqlite3": "^5.0.0",
    "mariadb": "^2.4.2",
    "postgres": "^2.0.0-beta.0"
  },
  "devDependencies": {
    "reflect-metadata": "^0.1.13"
