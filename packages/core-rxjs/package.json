{
  "name": "@super-hornet/core-rxjs",
  "version": "0.1.19",
  "description": "Super Hornet core rxjs library",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "module": "dist/",
  "repository": "https://github.com/super-hornet/super-hornet.ts",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --coverage",
    "tsc": "rm -rf dist && tsc",
    "tsc-watch": "rm -rf dist && tsc --watch"
  },
  "peerDependencies": {
    "@super-hornet/core": "^0.1.11",
    "rxjs": "~6.5.3"
  },
  "devDependencies": {
    "@super-hornet/core": "^0.1.19",
    "rxjs": "~6.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [],
    "testMatch": [
      "**/tests/**/*.+(ts|tsx|js)"
    ],
    "setupFilesAfterEnv": ["jest-extended"]
  },
  "globals": {
    "ts-jest": {
      "tsConfigFile": "tsconfig.json"
    }
  }
}
