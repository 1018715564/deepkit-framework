{
  "name": "@super-hornet/marshal",
  "version": "2.1.13",
  "description": "Marshal.ts the fastest universal TypeScript serializer and validator with JIT engine for frontend, backend, http-transport, RPC, and database, with built-in validations, NestJS support, and MongoDB ORM/Query builder, for Browser/NodeJS.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "jest --coverage",
    "tsc": "tsc",
    "tsc-watch": "rm -rf dist && tsc --watch"
  },
  "repository": "https://github.com/super-hornet/super-hornet",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "peerDependencies": {
    "moment": "^2.0.0"
  },
  "optionalDependencies": {
    "moment": "^2.0.0"
  },
  "dependencies": {
    "@super-hornet/core": "^0.1.19",
    "buffer": "^5.2.1",
    "clone": "^2.1.1",
    "get-parameter-names": "^0.3.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/moment": "^2.0.0",
    "moment": "^2.0.0"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/node_modules/**",
      "!**/lib/**/*",
      "!**/*.d.ts",
      "!index.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
