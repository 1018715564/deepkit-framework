{
  "name": "@deepkit/type",
  "version": "1.0.1-alpha.12",
  "description": "@deepkit/type, the fastest universal TypeScript serializer and validator with JIT engine for frontend, backend, http-transport, RPC, and database, with built-in validations, for Browser/NodeJS.",
  "type": "module",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo '{\"type\": \"commonjs\"}' > ./dist/cjs/package.json"
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "dependencies": {
    "@deepkit/core": "^1.0.1-alpha.12",
    "@types/validator": "^13.1.0",
    "buffer": "^5.2.1",
    "clone": "^2.1.1",
    "uuid": "^8.3.2",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "reflect-metadata": "^0.1.13"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  }
}
