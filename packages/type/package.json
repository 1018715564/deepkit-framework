{
  "name": "@deepkit/type",
  "version": "1.0.1-alpha.1",
  "description": "@deepkit/type, the fastest universal TypeScript serializer and validator with JIT engine for frontend, backend, http-transport, RPC, and database, with built-in validations, for Browser/NodeJS.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --coverage",
    "tsc": "tsc --build",
    "tsc-watch": "rm -rf dist && tsc --watch"
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "dependencies": {
    "@deepkit/core": "^1.0.1-alpha.1",
    "@types/validator": "^13.1.0",
    "buffer": "^5.2.1",
    "clone": "^2.1.1",
    "get-parameter-names": "^0.3.0",
    "uuid": "^3.2.1",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/jest": "^26.0.3",
    "reflect-metadata": "^0.1.13"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "extensionsToTreatAsEsm": [".ts", ".tsx"],
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  }
}
